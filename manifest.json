{
    "manifest_version": 3,
    "name": "RTSP Stream Redirector",
    "version": "1.0",
    "description": "Redirects RTSP link to cctvcameralive.in for live streaming.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "permissions": ["tabs"]
  }
  